{
  "manifest_version": 3,
  "name": "Web Auditor",
  "version": "1.0",
  "permissions": ["tabs", "scripting", "storage"],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "axe.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}